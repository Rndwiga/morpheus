{
	"name": "tyondo/laravel-mail",
	"description": "A simple website sms package",
	"keywords": ["sms", "tyondo"],
	"type": "library",
	"minimum-stability": "dev",
	"license": "MIT",
	"homepage": "https://github.com/Rndwiga/mnara",
	"authors": [
		{
			"name": "Raphael Ndwiga",
			"email": "packages@tyondo.com"
		}
	],
	"require": {
		"php": ">=5.6.4",
		"php-imap/php-imap": "~2.0",
		"illuminate/support": "5.3.*|5.4.*",
		"laravelcollective/html": "5.3.1|5.4.1",
		"guzzlehttp/guzzle": "^6.2",
		"infobip/infobip-api-php-client": "dev-master",
		"maatwebsite/excel": "^2.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.5"
	},
	"suggest": {
		"tyondo/mnara": "User management package with ACL for managing Users / Roles / Permissions in Laravel 5.3 and 5.4."
	},
	"autoload": {
		"psr-4": {
			"Tyondo\\Email\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Tyondo\\Email\\TyondoSmsServiceProvider"
			]
		}
	}
}

